    public IActionResult MenuOrderApp(int customerId)
    {
        var categories = _menuService.GetAllCategories();
        ViewBag.CustomerId = customerId;
        var table = _accountmanagerorderapp.GetTablesByCustomerId(customerId);
        
        var categoryvm = new MenuCategoryVM
        {
            CategoryName = categories.FirstOrDefault().CategoryName,
            customerTables = table.Select(sec => new SectionVM{
                SectionName = _tableandsectionService.GetSectionNameByTableId((int)sec.TableId),
                Tab = table.Select(tab => new SectionVM{
                    TableName = _tableandsectionService.GetTableNameByTableId((int)tab.TableId),
                }).ToList(),
            }).ToList(),
            menuCategories = categories
        };
        return View(categoryvm);
