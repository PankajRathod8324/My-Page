<script>
    $(document).ready(function () {
        // Show the first category by default
        $('.categoryBtn').click(function (){
            var categoryId = $(this).data('category');
            $.ajax({
                url: '@Url.Action("GetKOT", "AccountManagerOrderApp")',
                type: 'GET',
                data: { categoryId: categoryId },
                success: function (data) {
                    console.log("Category items fetched successfully:", data);
                    $('#kotcard').html(data);
                    $('#kotcard').show();
                },
                error: function (xhr, status, error) {
                    console.error("Error fetching category items:", error);
                }
            });
        });
    });
    function showCategory(categoryId) {
        // Hide all category contents
        document.querySelectorAll(".category-content").forEach(content => {
            content.style.display = "none";
        });

        if (categoryId === "All") {
            // Show all category cards
            document.getElementById("All").style.display = "block";
        } else {
            // Show only the selected category
            document.getElementById(categoryId).style.display = "block";
        }

        // Update tab styling
        document.querySelectorAll(".nav-link").forEach(tab => {
            tab.classList.remove("text-primary", "border-bottom", "border-primary");
            @* tab.classList.remove('active-link'); *@
                tab.classList.add("text-nav-color");
        });

        // Highlight the selected tab
        let activeTab = document.querySelector(`[data-category="${categoryId}"]`);
        activeTab.classList.add("text-primary", "border-bottom", "border-primary");
        @* activeTab.classList.add('active-link'); *@



            activeTab.classList.remove("text-nav-color");
    }
</script>

