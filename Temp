 $("#modifierGroupDropdown").change(function () {
                var groupId = $(this).val();
                var parentdiv = $('.min-input').val();
                @* var min = parentdiv.find('.min-input').val(); *@
                console.log(parentdiv);
                selectedModifierGroups.push({
                    modifiergroupid: groupId,
                });
                console.log(groupId);
                console.log(selectedModifierGroups);

                if (groupId) {
                    $.ajax({
                        url: '@Url.Action("GetModifiersByGroup", "Menu")', // Change ControllerName
                        type: 'GET',
                        data: { groupId: groupId },
                        success: function (response) {
                            $("#modifierList").append(response); // Update modifier list
                        }
                    });
                } else {
                    $("#modifierList").html(""); // Clear list if no group selected
                }
            });

